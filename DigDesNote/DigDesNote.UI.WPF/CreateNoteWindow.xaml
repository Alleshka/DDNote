<Window x:Class="DigDesNote.UI.WPF.CreateNoteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DigDesNote.UI.WPF"
        mc:Ignorable="d"
        Title="CreateNoteWindow" Height="320" Width="330">
    <Grid>
        <local:NoteControl Margin="0, 10, 0, 0" x:Name="_noteControl"></local:NoteControl>
        <Button Margin="0, 250, 0, 0" Height="25" Click="Button_Click">Создать заметку</Button>
    </Grid>
</Window>
