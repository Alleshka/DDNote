<Window x:Class="DigDesNote.UI.WPF.CreateNoteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DigDesNote.UI.WPF"
        mc:Ignorable="d"
        Title="CreateNoteWindow" Height="300" Width="300">
    <Grid>
        <local:NoteControl x:Name="_noteControl" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0, 15, 0, 0" Width="235"></local:NoteControl>
        <Button Margin="0, 75, 0, 0" Height="25" Click="Button_Click">Создать заметку</Button>
    </Grid>
</Window>
