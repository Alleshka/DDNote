<Window x:Class="DigDesNote.UI.WPF.AddCategoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DigDesNote.UI.WPF"
        mc:Ignorable="d"
        Title="AddCategoryWindow" Height="300" Width="300" Loaded="Window_Loaded" Closing="Window_Closing">
    <Grid>
        <ListBox MouseDoubleClick="AllCategory_MouseDoubleClick" Margin="0, 15, 0, 0" x:Name="AllCategory" Width="120" Height="150" HorizontalAlignment="Left" VerticalAlignment="Top"></ListBox>
        <ListBox MouseDoubleClick="AddedCategory_MouseDoubleClick" Margin="0, 15, 0, 0" x:Name="AddedCategory" Width="120" Height="150" HorizontalAlignment="Right" VerticalAlignment="Top"></ListBox>
    </Grid>
</Window>
